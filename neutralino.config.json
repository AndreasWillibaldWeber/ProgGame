{
  "applicationId": "ProgGame",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/proggame-react/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "filesystem.writeFile",
    "filesystem.readFile",
    "os.execCommand",
    "storage.*"
  ],
  "globalVariables": {
    "PY_CMD": "python3"
  },
  "modes": {
    "window": {
      "title": "ProgGame",
      "width": 1280,
      "height": 1024,
      "minWidth": 1280,
      "minHeight": 1024,
      "icon": "/proggame-react/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "ProgGame",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/proggame-react/build/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.11.0",
    "frontendLibrary": {
      "patchFile": "/proggame-react/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}
